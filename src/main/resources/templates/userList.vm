<div class="container-fluid">
    <div class="row-fluid">
        <div class="span12">
            <h3>用户列表</h3>
            <table class="table table-hover">
                <thead>
                    <tr>
                        <td>编号</td>
                        <td>昵称</td>
                        <td>等级</td>
                        <td>手机号码</td>
                        <td>积分</td>
                        <td>游戏币</td>
                        <td>注册时间</td>
                    </tr>
                </thead>
                <tbody>
                    #foreach($user in $list)
                    <tr>
                        <td>$user.id</td>
                        <td>$user.nickName</td>
                        <td>$user.userLevel</td>
                        <td>$user.phoneNumber</td>
                        <td>$user.userPoint</td>
                        <td>$user.gameMoney</td>
                        <td>$user.createTime</td>
                    </tr>
                    #end
                </tbody>
            </table>
